  1 -- 0..* Equipment;
  1 -- 0..* EquipmentBundle;
  Integer duration;
}

class Assignment{
  0..* -- 1 Member;
  1 -- 0..1 Guide;
  1 -- 0..1 Hotel;
  1 -- 0..* Equipment;
  1 -- 0..* EquipmentBundle;
  Integer startWeek;
  Integer duration;
  Integer price;
}
